{
  "name": "electron-text-to-image",
  "version": "1.0.0",
  "main": "main.js",
  "description": "A desktop application for generating images from text prompts using DALL-E API",
  "author": "Electron App Developer",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run start:electron\" \"npm run start:react\"",
    "start:electron": "wait-on http://localhost:3000 && electron .",
    "start:react": "vite",
    "build": "vite build && electron-builder",
    "lint": "eslint ."
  },
  "dependencies": {
    "@mui/material": "^5.15.10",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.10",
    "axios": "^1.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "^29.0.0",
    "electron-builder": "^24.9.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "typescript": "^5.4.2",
    "vite": "^5.2.0",
    "wait-on": "^7.2.0"
  }
}